{% extends "layout.nunjucks" %}

{% block content %}

<section>
	<div class="banner banner__home">
	</div>
</section>

<section class="block-list">
	{% for item in homeblocks %}
	<div class="block">
		<a href="{{item.url}}"><img src="{{item.img}}"/></a>
		<p>{{item.name}}</p>
	</div>
	{% endfor %}
</section>

{% endblock %}